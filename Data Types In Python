#list
[1,2,3]
Answer:[1, 2, 3]

['Cokie Dough', 'Strawberry', 'Chocolate']
['Cokie Dough', 'Strawberry', 'Chocolate']

['Vanilla',['Scoops', 'Spoon'], True]
['Vanilla', ['Scoops', 'Spoon'], True]

ice_cream=['Cokie Dough', 'Strawberry', 'Chocolate']
ice_cream.append('Salted Caramel')
ice_cream

['Cokie Dough', 'Strawberry', 'Chocolate', 'Salted Caramel']
['Cokie Dough', 'Strawberry', 'Chocolate', 'Salted Caramel']

ice_cream[0]='Butter Pecan'
ice_cream

['Butter Pecan', 'Strawberry', 'Chocolate', 'Salted Caramel']

['Vanilla',3,['Scoops','Spoon'],True]
['Vanilla', 3, ['Scoops', 'Spoon'], True]

nest_list=['Vanilla', 3, ['Scoops', 'Spoon'], True]
nest_list[2][1]
'Spoon'

nest_list[0]
'Vanilla'


nest_list[2]
['Scoops', 'Spoon']


nest_list[2][1]
'Spoon'
